// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Users {
  id Int @id @default(autoincrement())
  name String
  password String
  goals Goals[] 

}

model Goals {
  id Int @id @default(autoincrement()) 
  name String
  autorID Int
  autor Users @relation(fields: [autorID], references: [id])
  @@index([autorID])
}
